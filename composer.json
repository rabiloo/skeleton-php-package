{
  "name": "vendor-slug/package-slug",
  "description": "package-description",
  "keywords": [
    "vendor-slug",
    "package-slug"
  ],
  "homepage": "https://github.com/github-uri",
  "license": "MIT",
  "authors": [
    {
      "name": "author-name",
      "email": "author@domain.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "vimeo/psalm": "^4.11"
  },
  "autoload": {
    "psr-4": {
      "VendorNamespace\\PackageNamespace\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "format": "vendor/bin/phpcbf",
    "format:check": "vendor/bin/phpcs",
    "psalm": "vendor/bin/psalm"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
